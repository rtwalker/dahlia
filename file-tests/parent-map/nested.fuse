let a: ubit<32>[8][8];
let b: ubit<32>[8][8];

for (let i: ubit<4> = 0..8) {
  for (let j: ubit<4> = 0..8) {
    if (i % 2 == 0) {
       a[i][j] := 0;
    } else {
       a[i][j] := 1;
    }
  }
}

for (let i: ubit<4> = 0..8) {
  for (let j: ubit<4> = 0..8) {
    if (i % 2 == 0) {
       b[i][j] := 1;
    } else {
       b[i][j] := 0;
    }
  }
}